{
    "name": "cssd/instrument-distribution",
    "description": "Medical Instrument Distribution System Backend",
    "type": "project",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0",
        "laravel/sanctum": "^3.0",
        "maatwebsite/excel": "^3.1",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.12",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
